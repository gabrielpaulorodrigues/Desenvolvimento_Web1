    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@testing-library/dom": "^6.4.8",
    "@testing-library/jest-dom": "^6.4.8",
    "babel-jest": "^29.7.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "transformIgnorePatterns": [
